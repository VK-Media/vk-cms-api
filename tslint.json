{
	"extends": [
		"tslint:latest"
	],
	"rules": {
		"no-submodule-imports": false,
		"object-literal-sort-keys": false,
		"member-ordering": false,
		"member-access": false,
		"curly": false,
		"no-this-assignment": false,
		"no-console": false,
		"semicolon": false,
		"no-empty": false,
		"no-string-literal": false,
		"quotemark": false,
		"trailing-comma": false,
		"arrow-parens": false
	}
}
